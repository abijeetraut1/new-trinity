<script src="./../js/jquery-latest.js"></script>

<body>
    include ../headers/_header

    script(src="./../js/jquery-latest.js")
    script(src="./../js/forall.js")
    <div id="fb-root"></div>
    <div class="container">
        <div class="row">
            <div class="col-lg-1 col-md-1">

            </div>
            <div class="col-lg-5 col-md-5">
                <!-- <img src="img/freeShipping.png" width="70px" id="freeShipping" style="position:absolute; z-index:1000;display:none;" /> -->
                <div id="myCarousel" class="carousel slide" data-ride="carousel">
                    <!-- Indicators -->
                    <ol class="carousel-indicators">
                        <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
                        <li data-target="#myCarousel" data-slide-to="1"></li>
                    </ol>

                    //- <!-- Wrapper for slides -->
                    <div class="carousel-inner">
                        <div class="item active">
                            img(src=`/images/product/upload/tshirt/front/${products.front}.png` alt=`${products.title}_front`)#display_product_img_front
                        </div>

                        <div class="item">
                            img(src=`/images/product/upload/tshirt/back/${products.back}.png` alt=`${products.title}_back`)#display_product_img_back
                            //- <img src="/images/product/upload/tshirt/its-ok-1677940706916.png" alt="Cute Panda Shirt - Back" id="display_product_img_back">
                        </div>

                    </div>

                    //- <!-- Left and right controls -->
                    <a class="left carousel-control" href="#myCarousel" data-slide="prev" style="background: none;">
                        <span class="glyphicon glyphicon-chevron-left"></span>
                        <span class="sr-only">Previous</span>
                    </a>
                    <a class="right carousel-control" href="#myCarousel" data-slide="next" style="background: none;">
                        <span class="glyphicon glyphicon-chevron-right"></span>
                        <span class="sr-only">Next</span>
                    </a>
                </div>
            </div>
            <div class="col-lg-1 col-md-1">

            </div>
            <div class="col-lg-5 col-md-5">
                h2.title
                    b= products.title
                //- <h2 class="title"><b>=products.title</b></h2>
                p.description= products.description
                span(style="font-size:19px; color:#008080; display:inline-block; font-family: Montserrat, sans-serif;")#selling_price= `Rs ${products.price}`

                <p></p>
                <div style="height:10px;"></div>
                <div style="background:#ffffff; border:1px solid #edeff3; padding:10px 10px 15px 10px;"><label
                    | for="choose_colors" style="font-size:12px;">Colors:</label>
                    
                    // did not work paste here
                    
                    <br /><label for="choose_size" style="font-size:12px;">Size:</label><a href='#sizeModal'
                        | style='font-size:12px; margin-left:10px;' data-toggle='modal'>What's your size?</a><br />
                    <div id="choose_size">
                        <div style="display:inline-block; margin:5px; border: 1px solid #ccc; padding: .5rem 2rem 0 2rem; border-radius:5px; user-select: none;"
                            | class="size">S</div>
                        <div style="display:inline-block; margin:5px; border: 1px solid #ccc; padding: .5rem 2rem 0 2rem; border-radius:5px; user-select: none;"
                            | class="size">M</div>
                        <div style="display:inline-block; margin:5px; border: 1px solid #ccc; padding: .5rem 2rem 0 2rem; border-radius:5px; user-select: none;"
                            | class="size">L</div>
                        <div style="display:inline-block; margin:5px; border: 1px solid #ccc; padding: .5rem 2rem 0 2rem; border-radius:5px; user-select: none;"
                            | class="size">XL</div>
                        <div style="display:inline-block; margin:5px; border: 1px solid #ccc; padding: .5rem 2rem 0 2rem; border-radius:5px; user-select: none;"
                            | class="size">XXL</div>
                    </div><br /><label for="choose_qty" style="font-size:12px;">Quantity:</label><br />
                    <div id="choose_qty">
                        <div style="display:inline-block; margin:5px; border: 1px solid #ccc;  border-radius:50%; user-select: none;"
                            | class="qty">1
                        </div>
                        <div style="display:inline-block; margin:5px; border: 1px solid #ccc;  border-radius:50%; user-select: none;"
                            | class="qty">2
                        </div>
                        <div style="display:inline-block; margin:5px; border: 1px solid #ccc;  border-radius:50%; user-select: none;"
                            | class="qty">3
                        </div>
                        <div style="display:inline-block; margin:5px; border: 1px solid #ccc;  border-radius:50%; user-select: none;"
                            | class="qty">4
                        </div>
                        <div style="display:inline-block; margin:5px; border: 1px solid #ccc;  border-radius:50%; user-select: none;"
                            | class="qty">5
                        </div>
                    </div>
                </div>
                span#productid(style="display:none;")= products.id 
                a(href=`./${products.slug}/order` url="cart?product=Cute-Panda-Shirt" class="btn btn-success btn-block" style="border-radius:0px; padding:15px; font-size:16px;" id="addToCart")
                    <b>Buy Now</b></a>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-1 col-md-1">

            </div>
            <div class="col-lg-5 col-md-5 products-specs">
                <br /><br /><br />

                <br /><br />
                <h5><b>GUIDE</b></h5>
                <hr />
                <span>100% Original Product</span>
                <br /><br />
                <span>Regular Fit</span><br />
                <br />
                <span>15 Day Returns</span>
                <br /><br />
                <h5><b>Delivery Time</b></h5>
                <hr />
                <span>Get it delivered by <b>10th - 15th Mar</b><br /><br />Express or Rush shipping may be available
                    | depending upon the product and color selected.</span><br />
                <br />
                <h5><b>Material & Care</b></h5>
                <hr />
                <span>100% Cotton</span>
                <br /><br />
                <span>Machine-wash</span>
                <br /><br />
                //- <script type="text/javascript">
                //-     var ad_width = "728px";
                //-     var ad_height = "90px";
                //-     if (window.innerWidth < 728) {
                //-         ad_width = "320px";
                //-         ad_height = "100px";
                //-     }
                //- </script>
                <h5><b>Product ID</b></h5>
                <hr />
                span.product_id= products.slug 
                <br /><br />
                <h5><b>Tags</b></h5>
                <hr />
                span= products.tags 

            </div>
            <div class="col-lg-1 col-md-1">

            </div>
        </div>
        <div class="row">
            <div class="col-lg-1 col-md-1">

            </div>
            <div class="col-lg-5 col-md-5">
                <br />
                <h5><b>Share This</b></h5>
                <hr />
                //- <!-- AddToAny BEGIN -->
                <div class="a2a_kit a2a_kit_size_32 a2a_default_style" data-a2a-icon-color="#999"
                    | data-a2a-title="Hi guys, checkout my new design Cute Panda Shirt - TeeShopper 😍😍">
                    <a class="a2a_button_facebook"></a>
                    <a class="a2a_button_whatsapp"></a>
                    <a class="a2a_button_facebook_messenger"></a>
                    <a class="a2a_button_google_plus"></a>
                    <a class="a2a_button_twitter"></a>
                    <a class="a2a_button_linkedin"></a>
                    <a class="a2a_button_copy_link"></a>
                    <a class="a2a_dd" href="https://www.addtoany.com/share"></a>
                </div>
                //- <!-- AddToAny END -->
                <br /><br />
                <h5 id="more_products_label" style="display:none;"><b>More products</b></h5>
                <hr id="more_products_hr" style="display:none;" />
                <div class="more_products row">

                </div>


            </div>
            <div class="col-lg-1 col-md-1">

            </div>
        </div>
        <div class="row">
            <div class="col-lg-1 col-md-1">

            </div>
            <div class="col-lg-10 col-md-10" style="text-align:center">
                <br /><br />
                <!-- <img src="img/service_info_banner.png" width="100%" /> -->
                <br />
            </div>
            <div class="col-lg-1 col-md-1">

            </div>
        </div>
    </div>


    

    

   


    
    <div id="cartModal" class="modal fade" role="dialog">
        <div class="modal-dialog">

            <!-- Modal content-->
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <h4 class="modal-title">My Bag</h4>
                </div>
                <div class="modal-body cartProducts" style="height:400px; overflow-y:scroll">

                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-success continue_shopping" data-dismiss="modal"
                        | style="font-size:12px; border:none; background:#fff; color:#5cb85c;">Continue Shopping</button>
                    <a href="multiCheckout"><button type="button" class="btn btn-default" style="font-size:13px;">Secure
                        | Checkout <span class="glyphicon glyphicon-heart"></span></button></a>
                </div>
            </div>
        </div>
    </div>
</body>


<script src="/bootstrap/dist/js/bootstrap.min.js"></script>
